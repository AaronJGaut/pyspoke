#!/usr/bin/env python
import sys
import asyncio
import json
from spoke.client import Client

channel, msg_str = sys.argv[1:]
msg = json.loads(msg_str)

client = Client()

async def main():
    await client.run()
    val = await client.call(channel, msg)
    await val
    print(val.result())

asyncio.run(main())
